{
  "SavRoute": {
    "uri": "ArticlePage",
    "path": "/article",
    "childs": [
      {
        "uri": "ArticlePage.list",
        "path": "/article/list",
        "methods": [
          "GET"
        ]
      },
      {
        "uri": "ArticlePage.modify",
        "path": "/article/modify/:id?",
        "methods": [
          "GET"
        ]
      },
      {
        "uri": "ArticlePage.update",
        "path": "/article/update/:aid",
        "methods": [
          "POST"
        ]
      },
      {
        "uri": "ArticlePage.comment",
        "path": "/article/comment/:aid",
        "methods": [
          "POST"
        ]
      }
    ],
    "parents": [
      {
        "uri": "ArticlePage.view",
        "path": "/articles/:aid",
        "methods": [
          "GET"
        ]
      }
    ]
  },
  "VueRoute": {
    "component": "Article/Article",
    "path": "/article",
    "children": [
      {
        "component": "Article/ArticleList",
        "name": "ArticleList",
        "path": "list",
        "methods": [
          "GET"
        ]
      },
      {
        "component": "Article/ArticleView",
        "name": "ArticleView",
        "path": "/articles/:aid",
        "methods": [
          "GET"
        ]
      },
      {
        "component": "Article/ArticleModify",
        "name": "ArticleModify",
        "path": "modify/:id?",
        "methods": [
          "GET"
        ]
      },
      {
        "component": "Article/ArticleModify",
        "name": "ArticleUpdate",
        "path": "update/:aid",
        "methods": [
          "POST"
        ]
      }
    ]
  }
}