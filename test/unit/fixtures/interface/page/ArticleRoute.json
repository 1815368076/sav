{
  "SavRoute": {
    "route": {
      "uri": "Page/Article",
      "path": "/article"
    },
    "childs": [
      {
        "uri": "Page/Article/list",
        "path": "/article/list",
        "methods": [
          "GET"
        ]
      },
      {
        "uri": "Page/Article/modify",
        "path": "/article/modify/:id?",
        "methods": [
          "GET"
        ]
      },
      {
        "uri": "Page/Article/update",
        "path": "/article/update/:aid",
        "methods": [
          "POST"
        ]
      },
      {
        "uri": "Page/Article/comment",
        "path": "/article/comment/:aid",
        "methods": [
          "POST"
        ]
      }
    ],
    "root": [
      {
        "uri": "Page/Article/view",
        "path": "/articles/:aid",
        "methods": [
          "GET"
        ]
      }
    ]
  },
  "VueRoute": {
    "component": "Article/Article",
    "path": "/article",
    "children": [
      {
        "component": "Article/ArticleList",
        "name": "ArticleList",
        "path": "list",
        "methods": [
          "GET"
        ]
      },
      {
        "component": "Article/ArticleView",
        "name": "ArticleView",
        "path": "/articles/:aid",
        "methods": [
          "GET"
        ]
      },
      {
        "component": "Article/ArticleModify",
        "name": "ArticleModify",
        "path": "modify/:id?",
        "methods": [
          "GET"
        ]
      },
      {
        "component": "Article/ArticleModify",
        "name": "ArticleUpdate",
        "path": "update/:aid",
        "methods": [
          "POST"
        ]
      }
    ]
  }
}