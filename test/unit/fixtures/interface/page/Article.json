{
  "moduleName": "Article",
  "moduleGroup": "Page",
  "view": "vue",
  "layout": "User",
  "uri": "Page/Article",
  "routes": [
    {
      "uri": "Page/Article/list",
      "actionName": "list",
      "plugins": [
        "route"
      ],
      "props": {
        "route": {
          "methods": [
            "GET"
          ]
        }
      }
    },
    {
      "uri": "Page/Article/view",
      "actionName": "view",
      "plugins": [
        "route"
      ],
      "props": {
        "route": {
          "methods": [
            "GET"
          ],
          "path": "/articles/:aid"
        }
      }
    },
    {
      "uri": "Page/Article/modify",
      "actionName": "modify",
      "plugins": [
        "auth",
        "route",
        "vue"
      ],
      "props": {
        "auth": null,
        "route": {
          "methods": [
            "GET"
          ],
          "path": "modify/:id?"
        },
        "vue": {
          "component": "Article/ArticleModify"
        }
      }
    },
    {
      "uri": "Page/Article/update",
      "actionName": "update",
      "plugins": [
        "auth",
        "route",
        "vue"
      ],
      "props": {
        "auth": null,
        "route": {
          "methods": [
            "POST"
          ],
          "path": "update/:aid"
        },
        "vue": {
          "component": "Article/ArticleModify"
        }
      }
    },
    {
      "uri": "Page/Article/comment",
      "actionName": "comment",
      "plugins": [
        "route",
        "vue"
      ],
      "props": {
        "route": {
          "methods": [
            "POST"
          ],
          "path": "comment/:aid"
        },
        "vue": false
      }
    }
  ]
}