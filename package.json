{
  "name": "sav",
  "version": "0.0.2",
  "description": "sav",
  "main": "dist/sav.cjs.js",
  "module": "dist/sav.es.js",
  "jsnext:main": "dist/sav.es.js",
  "scripts": {
    "watch": "watch -p \"+(test|src)/**/*.js\" -c \"npm run fix && npm test\"",
    "ws": "watch -p \"+(src)/**/*.js\" -c \"npm run fix && npm start\"",
    "start": "node ./server",
    "build": "rollup -c && node .",
    "lint": "standard rollup.config.js test/** src/**",
    "fix": "npm run lint -- --fix",
    "na": "nyc ava ./test/unit",
    "pretest": "npm run lint",
    "test": "npm run na",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/savjs/sav.git"
  },
  "author": {
    "name": "jetiny",
    "email": "86287344@qq.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/savjs/sav/issues"
  },
  "devDependencies": {
    "ava": "^0.18.2",
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.1.1",
    "babel-plugin-resolver": "^1.1.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.23.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "bluebird": "^3.5.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "eslint-plugin-standard": "^2.0.1",
    "handlebars": "^4.0.6",
    "koa-compose": "^3.2.1",
    "nyc": "^10.0.0",
    "path-to-regexp": "^1.7.0",
    "rollup": "^0.41.4",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-commonjs": "^8.0.0",
    "rollup-plugin-includepaths": "^0.1.8",
    "rollup-plugin-json": "^2.1.0",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-plugin-re": "^1.0.1",
    "rollup-plugin-vue2": "^0.8.0",
    "sav-schema": "0.0.8",
    "sav-util": "0.0.9",
    "standard": "^9.0.1",
    "vue": "^2.2.4",
    "vue-router": "^2.3.0",
    "vue-server-renderer": "^2.2.4",
    "vue-template-compiler": "^2.2.4",
    "watch-cli": "^0.2.1"
  },
  "dependencies": {
    "bluebird": "^3.4.7",
    "htmling": "0.0.8",
    "consolidate": "^0.14.5"
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "fixtures",
      "package.json"
    ],
    "global": [
      "AsyncFunction"
    ]
  },
  "nyc": {
    "exclude": [
      "node_modules",
      "test"
    ]
  },
  "ava": {
    "require": "babel-register",
    "babel": "inherit"
  },
  "babel": {
    "plugins": [
      [
        "resolver",
        {
          "resolveDirs": [
            "src"
          ]
        }
      ],
      "transform-object-rest-spread",
      "transform-decorators-legacy",
      "transform-es2015-modules-commonjs"
    ]
  },
  "homepage": "https://github.com/savjs/sav#readme"
}
