{
  "name": "sav",
  "version": "0.0.8",
  "description": "sav",
  "main": "dist/sav.js",
  "scripts": {
    "watch": "watch -p \"+(test|src)/**/*.js\" -c \"npm run fix && npm run na\"",
    "wc": "watch -p \"+(src)/**/*.js\" -c \"npm run build-cli && npm run test-cli\"",
    "wb": "watch -p \"+(src)/**/*.js\" -c \"npm run build\"",
    "start": "node ./server",
    "build": "npm run build-sav && npm run build-cli && npm run build-client",
    "build-sav": "rollup -c scripts/rollup.config.js",
    "build-cli": "rollup -c scripts/rollup.cli.js",
    "build-client": "rollup -c scripts/rollup.client.js",
    "lint": "standard rollup.config.js test/** src/**",
    "fix": "npm run lint -- --fix",
    "cli": "node ./dist/sav-cli",
    "cv": "node ./dist/sav-cli.js -p ./test/unit/fixtures/interface -d ./test/unit/fixtures/contract -a ./test/unit/fixtures/action -v ./test/unit/fixtures/view",
    "na": "nyc ava ./test/unit/**",
    "naw": "nyc ava ./test/unit/plugins/action.js",
    "pretest": "npm run lint",
    "test": "npm run na",
    "nit": "npm-install-tag",
    "prepare": "npm run build"
  },
  "bin": {
    "sav-cli": "bin/sav-cli"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/savjs/sav.git"
  },
  "author": {
    "name": "jetiny",
    "email": "86287344@qq.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/savjs/sav/issues"
  },
  "devDependencies": {
    "acorn": "^5.0.3",
    "ava": "^0.18.2",
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.1.1",
    "babel-plugin-resolver": "^1.1.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.23.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-standalone": "^6.24.0",
    "bluebird": "^3.4.7",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "commander": "^2.9.0",
    "consolidate": "^0.14.5",
    "eslint-plugin-standard": "^2.0.1",
    "fs-extra": "^3.0.1",
    "handlebars": "^4.0.6",
    "htmling": "0.0.8",
    "koa-compose": "^3.2.1",
    "npm-install-tag": "^1.0.4",
    "nyc": "^10.0.0",
    "path-to-regexp": "^1.7.0",
    "rollup": "^0.41.4",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-commonjs": "^8.0.0",
    "rollup-plugin-includepaths": "^0.1.8",
    "rollup-plugin-json": "^2.1.0",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-plugin-re": "^1.0.1",
    "rollup-plugin-vue2": "^0.8.0",
    "sav-schema": "0.0.17",
    "sav-util": "^1.0.21",
    "standard": "^9.0.1",
    "vue": "^2.2.4",
    "vue-router": "^2.3.0",
    "vue-server-renderer": "^2.2.4",
    "vue-template-compiler": "^2.2.4",
    "watch-cli": "^0.2.1"
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "fixtures",
      "package.json"
    ],
    "global": [
      "AsyncFunction"
    ]
  },
  "nyc": {
    "exclude": [
      "node_modules",
      "test"
    ]
  },
  "ava": {
    "require": "babel-register",
    "babel": "inherit"
  },
  "babel": {
    "plugins": [
      [
        "resolver",
        {
          "resolveDirs": [
            "src"
          ]
        }
      ],
      "transform-object-rest-spread",
      "transform-decorators-legacy",
      "transform-es2015-modules-commonjs"
    ]
  },
  "homepage": "https://github.com/savjs/sav#readme"
}
